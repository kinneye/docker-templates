<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/kinneye/docker-templates/master/epkinney/oracle.xml</TemplateURL>
  <Beta>False</Beta>
  <Category></Category>
  <Name>Oracle</Name>
  <Description>Description
  </Description>
  <Overview>Overview
  </Overview>
  <Support></Support>
  <Registry>https://hub.docker.com/u/epkinney/content/sub-94fcc980-0414-487e-9b29-dd63f3d6e181</Registry>
  <GitHub>https://github.com/kinneye/oracle</GitHub>
  <Repository>store/oracle/database-enterprise:12.2.0.1</Repository>
  <Project></Project>
  <Deprecated>false</Deprecated>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>1521</HostPort>
        <ContainerPort>1521</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Config
    Type="Variable" 
    Name="ORACLE_SID" 
    Target="DB_SID" 
    Default="ORCLCDB" 
    Description="This parameter changes the ORACLE_SID of the database. The default value is set to ORCLCDB." 
    Mask="false" 
    Display="always" 
    Required="false">ORCLCDB</Config>
  <Config
    Type="Variable" 
    Name="DB_PDB" 
    Target="DB_PDB" 
    Default="ORCLPDB1" 
    Description="This parameter modifies the name of the PDB. The default value is set to ORCLPDB1." 
    Mask="false" 
    Display="always" 
    Required="false">ORCLPDB1</Config>
  <Config
    Type="Variable" 
    Name="DB_MEMORY" 
    Target="DB_MEMORY" 
    Default="2GB" 
    Description="This parameter sets the memory requirement for the Oracle server. This value determines the amount of memory to be allocated for SGA and PGA. The default value is set to 2GB." 
    Mask="false" 
    Display="always" 
    Required="false">2GB</Config>
    <Config
    Type="Variable" 
    Name="DB_DOMAIN" 
    Target="DB_DOMAIN" 
    Default="localdomain" 
    Description="This parameter sets the domain to be used for database server. The default value is localdomain."
    Mask="false" 
    Display="always" 
    Required="false">localdomain</Config>
  <Environment>
  </Environment>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/ORCL/OracleDBData</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Icon>https://github.com/kinneye/docker-templates/raw/master/epkinney/images/oracle.png</Icon>
  <ExtraParams></ExtraParams>
</Containers>
