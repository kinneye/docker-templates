<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/kinneye/docker-templates/master/epkinney/deployment-center.xml</TemplateURL>
  <Beta>False</Beta>
  <Category></Category>
  <Name>Deployment_Center</Name>
  <Description>Description
  </Description>
  <Overview>Overview
  </Overview>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/epkinney/deployment_center/</Registry>
  <GitHub>https://github.com/kinneye/deployment_center</GitHub>
  <Repository>epkinney/deployment_center</Repository>
  <Project></Project>
  <Deprecated>true</Deprecated>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>28000</HostPort>
        <ContainerPort>28000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>Deployment_Center_Version</Name>
      <Value>2.1.1</Value>
    </Variable>
	<Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/deployment_center/repo</HostDir>
      <ContainerDir>/repo</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
	<Volume>
      <HostDir>/mnt/user/deployment_center/install_media</HostDir>
      <ContainerDir>/install_media</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Icon>https://github.com/kinneye/docker-templates/raw/master/epkinney/images/lmtools_129.png</Icon>
  <ExtraParams></ExtraParams>
</Containers>
