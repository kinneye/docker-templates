<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/kinneye/docker-templates/master/epkinney/splm-license-server.xml</TemplateURL>
  <Beta>False</Beta>
  <Category></Category>
  <Name>splm-license-server.xml</Name>
  <Description>Description
  </Description>
  <Overview>Overview
  </Overview>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/epkinney/splm-license-server/</Registry>
  <GitHub>https://github.com/kinneye/splm_license_server</GitHub>
  <Repository>epkinney/splm-license-server</Repository>
  <Project></Project>
  <Deprecated>true</Deprecated>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>28000</HostPort>
        <ContainerPort>28000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>SPLMLicenseServer_Version</Name>
      <Value>9.0.1</Value>
    </Variable>
    <Variable>
      <Name>SPLMLicense_FILE</Name>
      <Value>splm9.lic</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/license</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Icon>https://github.com/kinneye/docker-templates/raw/master/epkinney/images/lmtools_129.png</Icon>
  <ExtraParams></ExtraParams>
</Containers>
